{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        ":prImmediately",
        ":prHourlyLimitNone",
        ":prConcurrentLimitNone",
        ":automergeDisabled",
        ":enableVulnerabilityAlertsWithLabel(security)",
        ":separateMultipleMajorReleases"
    ],
    "baseBranches": [
        "main"
    ],
    "enabledManagers": [
        "nuget"
    ]
}
